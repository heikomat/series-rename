{
  "extends": "5minds-typescript/fast",
  "rules": {
    "@typescript-eslint/no-explicit-any": [
      "warning"
    ],
    "@typescript-eslint/no-extraneous-class": "off"
  },
  "globals": {
    "localStorage": true,
    "fetch": true,
    "window": true,
    "document": true,
    "navigator": true,
    "jasmine": true,
    "expect": true,
    "MutationObserver": true,
    "requestAnimationFrame": true
  }
}
